{
  "manifest_version": 2,
  "name": "Count Me",
  "description": "Displays and counts all open tabs.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
    } ,
  "permissions": [
    "tabs"
  ],
  "browser_action": {
	  "default_popup": "popup.html",
	  "default_icon": "icon.png"
  }
}